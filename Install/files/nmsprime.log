/var/www/nmsprime/storage/logs/laravel.log {
    weekly
    missingok
    rotate 6
#   compress
#   delaycompress
    notifempty
    create 644 apache apache
    postrotate
        /opt/rh/rh-php73/root/usr/bin/php artisan queue:restart || true
        /opt/rh/rh-php73/root/usr/bin/php artisan websockets:restart || true
    endscript
}

/var/www/nmsprime/storage/logs/billing.log
/var/www/nmsprime/storage/logs/bank-transactions.log
{
    monthly
    missingok
    rotate 6
    notifempty
    create 644 apache apache
    postrotate
        /opt/rh/rh-php73/root/usr/bin/php artisan queue:restart || true
    endscript
}

/var/www/nmsprime/storage/logs/worker.log {
    monthly
    missingok
    rotate 3
    notifempty
    create 644 root apache
    postrotate
        /opt/rh/rh-php73/root/usr/bin/php artisan queue:restart || true
    endscript
}
