{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "laravel/framework": "^8.0",
        "laravelcollective/html": "6.*",
        "digitick/sepa-xml": "1.*",
        "globalcitizen/php-iban": "4.*",
        "box/spout": "^3.0",
        "nmsprime/madzipper": "1.1.*",
        "yajra/laravel-datatables-oracle": "9.*",
        "markrogoyski/ipv4-subnet-calculator": "3.*",
        "nwidart/laravel-modules": "^7.0",
        "doctrine/dbal": "^2.7",
        "silber/bouncer": "v1.0.0",
        "kingsquare/php-mt940": "^2.0",
        "fideloper/proxy": "^4.4",
        "phpseclib/phpseclib": "~2.0",
        "intervention/validation": "^2.6",
        "kalnoy/nestedset": "^6.0",
        "beyondcode/laravel-websockets": "^1.13",
        "pusher/pusher-php-server": "~4.0",
        "laravel/ui": "^3.0",
        "laravel/legacy-factories": "^1.3"
    },
    "require-dev": {
        "swen100/geophp": "^1.0",
        "fruitcake/laravel-cors": "^3.0",
        "laravel/tinker": "^2.5",
        "facade/ignition": "^2.3.6",
        "fakerphp/faker": "^1.9.1",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "classmap": [
            "database",
            "database/migrations",
            "database/seeds"
        ],
        "psr-4": {
            "App\\": "app/",
            "Modules\\": "modules",
            "Acme\\": "app/extensions/"
        },
        "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php",
            "tests/BaseLifecycleTest.php"
        ]
    },
    "scripts": {
        "post-install-cmd": [
            "php artisan clear-compiled"
        ],
        "post-update-cmd": [
            "php artisan clear-compiled"
        ],
        "post-create-project-cmd": [
            "php -r \"copy('.env.example', '.env');\"",
            "php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "optimize-autoloader": true
    }
}
